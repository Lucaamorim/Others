import java.util.Scanner;

public class ProjetosLL {

    public static void main(String[] args) {
        
        int n1 =0, n2 =1,n3 = 0, seuNumero;
        
        Scanner digite = new Scanner(System.in);
        
        System.out.println("Digite um numero inteiro e verifique se ele pertece a sequencia de Fibonacci:");
        
        seuNumero = digite.nextInt();
        
    while (seuNumero > n3){
        n3 = n1 + n2;
        n1 = n2;
        n2 = n3;
    }
    
    if (seuNumero == 0){
    System.out.println("Seu número pertence a sequencia  de Fibonacci");
    } else if (seuNumero == n3){
     System.out.println("Seu número pertence a sequencia  de Fibonacci");
    } else {
     System.err.println("Numero nao indentificado na sequencia  de Fibonacci");
    }
    
    }
}
